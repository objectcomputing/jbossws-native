<?xml version="1.0" encoding="UTF-8"?>

<application-client xmlns="http://java.sun.com/xml/ns/j2ee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/application-client_1_4.xsd"
  version="1.4">

  <display-name>TestService</display-name>

  <ejb-ref>
    <ejb-ref-name>ejb/HelloEJBTwo</ejb-ref-name>
    <ejb-ref-type>Session</ejb-ref-type>
    <home>org.jboss.test.ws.jaxrpc.jbws772.HelloHome</home>
    <remote>org.jboss.test.ws.jaxrpc.jbws772.HelloRemote</remote>
  </ejb-ref>
  
  <service-ref>
    <service-ref-name>service/TestService</service-ref-name>
    <service-interface>javax.xml.rpc.Service</service-interface>
    <wsdl-file>META-INF/wsdl/TestService.wsdl</wsdl-file>
    <jaxrpc-mapping-file>META-INF/jaxrpc-mapping.xml</jaxrpc-mapping-file>
    <port-component-ref>
      <service-endpoint-interface>org.jboss.test.ws.jaxrpc.jbws772.Hello</service-endpoint-interface>
    </port-component-ref>
  </service-ref>
  

</application-client>

