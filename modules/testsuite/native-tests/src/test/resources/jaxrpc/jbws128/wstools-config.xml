   <service name="HelloService">
     <port name="HelloPort" binding="tns:HelloBinding">
+      <soap:address location="http://@jboss.bind.address@:8080/jaxrpc-jbws128-service"/>
-      <soap:address location="REPLACE_WITH_ACTUAL_URL"/>
     </port>
   </service>
